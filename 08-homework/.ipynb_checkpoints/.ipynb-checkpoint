    "article_list = []\n",
    "\n",
    "\n",
    "for row in rows: \n",
    "    article = {}\n",
    "    article['headline'] = row.text\n",
    "    article_string = row.attrs['onclick']\n",
    "    article['ID'] = re.findall('AR[0-9]+', article_string)[0]\n",
    "    article_list.append(article)\n",
    "    \n",
    "   "
